
public class Location
{
    public int xCoord;
    public int yCoord;

    public Location(int x, int y)
    {
        xCoord = x;
        yCoord = y;
    }

    public Location()
    {
        this(0, 0);
    }
    
    public boolean equals(Object obj) {
    	if (obj == this) {
    		return true;
    	}
		if (obj == null) {
			return false;
		}
    	Location a = (Location) obj;
    	return this.xCoord == a.xCoord && this.yCoord == a.yCoord;
    }
    
    public int hashCode()
    {
		final int prime = 31;
		int result = 1;
		result = prime * result + ;
		result = prime * result + varB;
		return result;
    }
}